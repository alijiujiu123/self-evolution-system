{
  "name": "openclaw-self-evolution-system",
  "version": "1.0.0",
  "description": "OpenClaw Self-Evolution System - An intelligent AI system that automatically learns, analyzes, optimizes, and deploys improvements to the OpenClaw ecosystem",
  "main": "index.js",
  "scripts": {
    "start": "cd skill && node index.cjs",
    "learn": "cd skill && node index.cjs learn",
    "optimize": "cd skill && node index.cjs optimize",
    "apply": "cd skill && node index.cjs apply",
    "report": "cd skill && node index.cjs report",
    "status": "cd skill && node index.cjs status",
    "monitor": "./evolution-monitor.sh",
    "quick-monitor": "./evolution-quick-monitor.sh",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openclaw/self-evolution-system.git"
  },
  "keywords": [
    "openclaw",
    "self-evolution",
    "ai",
    "automation",
    "optimization",
    "machine-learning"
  ],
  "author": "OpenClaw",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  },
  "bugs": {
    "url": "https://github.com/openclaw/self-evolution-system/issues"
  },
  "homepage": "https://github.com/openclaw/self-evolution-system#readme"
}
